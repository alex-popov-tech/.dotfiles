# vi: ft=zsh

# vi-mode go to different modes faster
export KEYTIMEOUT=1
# add brew and packages to PATH
export PATH="/usr/local/bin:/opt/homebrew/bin:$PATH:$HOME/.local/bin"
# Preferred editor for local sessions
export EDITOR="nvim"
# LANG=""
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8
# for gpg
export GPG_TTY=$TTY

export GOROOT="$HOME/.asdf/installs/golang/1.20.5/go"
export GOPATH="$HOME/me/go"
export PATH="$PATH:$GOPATH/bin:$GOROOT/bin"
