Host router
  HostName 192.168.31.1
  User admin
  PubKeyAuthentication yes
  IdentityFile ~/.ssh/id_rsa.personal
Host github.com
  User git
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa.personal
Host gitlab.com
  IdentityFile ~/.ssh/id_rsa.gitlab.personal
Host *
  IdentityFile ~/.ssh/id_rsa.personal
